{
  "name": "jcc2d-flycoins",
  "version": "0.0.2",
  "description": "jcc2d flycoins fly star stars",
  "main": "build/index.js",
  "scripts": {
    "release": "gulp release",
    "build": "gulp build",
    "watch": "gulp build --watch",
    "prever": "npm run release && git add -A && git commit -m 'release: release new version'",
    "ver": "standard-version -m 'chore(release): v%s'",
    "postver": "git push --follow-tags origin master && npm publish",
    "lint": "eslint .",
    "fix": "eslint --fix ./src",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasonChen1982/jcc2d-flycoins.git"
  },
  "keywords": [
    "jcc2d",
    "flycoins",
    "fly",
    "star",
    "stars"
  ],
  "author": {
    "name": "jason Chen",
    "email": "jasonchen1882@gmail.com",
    "url": "http://jason82.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonChen1982/jcc2d-flycoins/issues"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "husky": "^0.13.2",
    "rollup": "^0.36.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^2.5.0",
    "standard-version": "^4.0.0",
    "validate-commit-msg": "^2.11.1"
  },
  "homepage": "https://github.com/jasonChen1982/jcc2d-flycoins#readme",
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "release",
        "build",
        "revert",
        "deps"
      ]
    }
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "chalk": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "yargs": "^8.0.2"
  }
}
